<?xml version="1.0"  encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="NHibernate.demo.Entity.TimesheetEntries, NHibernate.demo.Entity" table="TimesheetEntries">
		<id name="TimesheetEntryId" column="TimesheetEntryId" type="int" unsaved-value="0">
      			<generator class="native" />       			
      	</id>      		
		<property name="EntryDate" column="EntryDate" type="DateTime"  />
		<property name="NumberOfHours" column="NumberOfHours" type="int"  />
		<property name="Comments" column="Comments" type="string"  />
		<property name="TimesheetID" column="TimesheetID" type="int"  />
				
	</class>
</hibernate-mapping>