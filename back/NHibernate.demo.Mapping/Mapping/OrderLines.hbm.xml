<?xml version="1.0"  encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="NHibernate.demo.Entity.OrderLines, NHibernate.demo.Entity" table="OrderLines">
		<id name="OrderLineId" column="OrderLineId" type="long" unsaved-value="0">
      			<generator class="native" />       			
      	</id>      		
		<property name="OrderId" column="OrderId" type="int"  />
		<property name="ProductId" column="ProductId" type="int"  />
		<property name="UnitPrice" column="UnitPrice" type="decimal"  />
		<property name="Quantity" column="Quantity" type="int"  />
		<property name="Discount" column="Discount" type="decimal"  />
				
	</class>
</hibernate-mapping>